{
  "name": "@dylanjs/utils",
  "version": "0.2.7",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "cm": "git add . && dy git-commit -l=zh-cn",
    "publish-pkg": "npm publish --access public",
    "release": "dy release && pnpm build && pnpm publish-pkg"
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@dylanjs/cli": "^1.0.4",
    "@types/node": "^22.15.3",
    "@types/qs": "^6.9.18",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@unocss/core": "^66.5.1",
    "@unocss/preset-uno": "^66.5.1",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "colord": "^2.9.3",
    "localforage": "^1.10.0",
    "nanoid": "^5.1.5",
    "qs": "^6.14.0"
  }
}
