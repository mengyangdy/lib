{
  "name": "@dylanjs/utils",
  "version": "0.2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "cm": "git add . && dy git-commit",
    "publish-pkg": "npm publish --access public",
    "release": "git add . && dy release && pnpm build && pnpm publish-pkg"
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/qs": "^6.9.18",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "dependencies": {
    "@unocss/core": "^66.3.2",
    "@unocss/preset-uno": "^66.3.2",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "colord": "^2.9.3",
    "localforage": "^1.10.0",
    "nanoid": "^5.1.5",
    "qs": "^6.14.0"
  }
}
